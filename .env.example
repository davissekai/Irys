# Backend runtime
EXTRACT_TIMEOUT_SECONDS=120

# CORS (comma-separated exact origins)
FRONTEND_ORIGINS=http://127.0.0.1:5173,http://localhost:5173,http://127.0.0.1:4174,http://localhost:4174,https://your-app.vercel.app
# Optional regex for Vercel preview deploys
FRONTEND_ORIGIN_REGEX=https://.*\.vercel\.app

# OCR provider (GLM-only pipeline)
GLM_OCR_API_KEY=
GLM_OCR_BASE_URL=https://api.z.ai/api/paas/v4/layout_parsing
GLM_OCR_MODEL=glm-ocr

# Optional LLM mapping
OPENROUTER_API_KEY=
OPENROUTER_MODEL=stepfun/step-3.5-flash

# Database
DATABASE_URL=postgresql+psycopg2://postgres:password@db.<project-ref>.supabase.co:5432/postgres
